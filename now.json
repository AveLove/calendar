{
  "routes": [
    {
      "src": "^/index.html(.*)",
      "headers": {
        "Clear-Site-Data": "\"cache\", \"storage\""
      },
      "dest": "/index.html$1"
    },
    {
      "src": "^/?(.+)",
      "headers": {
        "Clear-Site-Data": "\"cache\", \"storage\""
      },
      "dest": "^/?$1"
    },
    {
      "src": "^/$",
      "headers": {
        "Clear-Site-Data": "\"cache\", \"storage\""
      },
      "dest": "/"
    }
  ]
}
